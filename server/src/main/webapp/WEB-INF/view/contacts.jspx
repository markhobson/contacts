<html xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:c="http://java.sun.com/jsp/jstl/core">

	<jsp:directive.page contentType="text/html"/>
	<jsp:output omit-xml-declaration="true" doctype-root-element="html" doctype-system="about:legacy-compat"/>
	
	<head>
		<title>Contacts</title>
	</head>
	<body>
		<h1>Contacts</h1>
		<ul>
			<c:forEach var="contact" items="${contacts}">
				<li>
					<c:out value="${contact.name}"/>
				</li>
			</c:forEach>
		</ul>
	</body>
	
</html>
