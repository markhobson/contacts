{{<../page}}
	{{$title}}Contacts{{/title}}
	{{$self}}/contacts{{/self}}
	{{$container}}
		<h1>Contacts</h1>
		
		<table class="table table-striped">
			<thead>
				<tr>
					<th>Name</th>
				</tr>
			</thead>
			<tbody>
				{{#contacts}}
					<tr itemscope="itemscope" itemtype="http://schema.org/Person">
						<td itemprop="name">
							<a rel="item" href="{{id.href}}" itemprop="url">{{content.name}}</a>
						</td>
					</tr>
				{{/contacts}}
			</tbody>
		</table>
		
		<div>
			<a rel="create-form" href="/contacts/create" class="btn btn-success">Create</a>
		</div>
	{{/container}}
{{/../page}}
